<Page x:Class="Ddm.WpfApplication.Pages.FileViewPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="FileViewPage">

    <Grid>
        <Button Name="ButtonGoLeft"  Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="ButtonGoLeft_Click"/>
        <Button Name="ButtonGoRight" Content="Button" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="75" Click="ButtonGoRight_Click"/>
        <Button Name="ButtonAddTag" Content="Button" HorizontalAlignment="Left" Margin="10,57,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAddTag_Click"/>
        <Button Name="ButtonAddComment" Content="Button" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAddComment_Click"/>
        <Button Name="ButtonRemoveTag" Content="Button" HorizontalAlignment="Left" Margin="10,105,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAddComment_Click"/>

    </Grid>
</Page>
